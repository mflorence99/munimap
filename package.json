    "@angular/forms": "^12",
    "@angular/material": "^12",
    "@angular/platform-browser": "^12",
    "@angular/platform-browser-dynamic": "^12",
    "@angular/router": "^12",
    "@ngxs/storage-plugin": "^3",
    "@ngxs/store": "^3",
    "@sentry/angular": "^6",
    "@sentry/tracing": "^6",
    "firebase": "^9",
    "material-design-icons": "^3",
    "rxfire": "^6",
    "rxjs": "^7",
    "tslib": "^2",
    "ua-parser-js": "^0.7.28",
    "zone.js": "~0.11.4"
