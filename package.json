{
  "name": "munimap",
  "version": "0.0.0",
  "description": "Create town and village maps in the style of washington-app",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mflorence99/munimap.git"
  },
  "author": "Mark Florence",
  "scripts": {
    "clean:deps": "rm -rf node_modules && rm package-lock.json && npm install",
