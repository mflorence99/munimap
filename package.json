{
  "name": "munimap",
  "version": "0.0.0",
  "description": "MuniMap",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mflorence99/munimap.git"
  },
  "author": "Mark Florence",
  "scripts": {
    "clean:deps": "rm -rf node_modules && rm -f package-lock.json && npm install",
    "emulators": "firebase emulators:start",
    "patcher": "ts-node --dir bin patcher.ts",
    "postinstall": "npm run patcher && ngcc",
    "prettier": "prettier --write .",
    "proxy:counties": "ts-node --dir bin counties.ts",
    "proxy:explorer": "ts-node --dir bin explorer.ts",
    "proxy:indexer": "ts-node --dir bin indexer.ts",
    "proxy:lakes": "ts-node --dir bin lakes.ts",
    "proxy:parcels": "node --max-old-space-size=8192 -- node_modules/.bin/ts-node --dir bin parcels.ts",
    "proxy:places": "ts-node --dir bin places.ts",
    "proxy:powerlines": "ts-node --dir bin powerlines.ts",
    "proxy:powerlines2": "ts-node --dir bin powerlines2.ts",
    "proxy:railroads": "ts-node --dir bin railroads.ts",
    "proxy:rivers": "ts-node --dir bin rivers.ts",
    "proxy:roads": "ts-node --dir bin roads.ts",
    "proxy:towns": "ts-node --dir bin towns.ts",
    "proxy:trails": "ts-node --dir bin trails.ts",
    "proxy:washington-buildings": "ts-node --dir bin washington-buildings.ts",
    "proxy:washington-parcels": "ts-node --dir bin washington-parcels.ts",
    "serve:author": "ng serve author --port 4200",
    "serve:proxy": "tsc -p proxy/src && node dist/proxy/server.js",
    "serve:viewer": "ng serve viewer --port 4202"
  },
